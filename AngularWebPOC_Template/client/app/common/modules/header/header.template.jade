.navbar.navbar-default.navbar-fixed-top(role='navigation', ng-init="navCollapsed = true")
  .container
    .navbar-header
      button.navbar-toggle.collapsed(type='button', ng-click="navCollapsed = !navCollapsed")
        span.sr-only Toggle navigation   
        span.icon-bar
        span.icon-bar 
        span.icon-bar
      a.navbar-brand(href='#') ng-multi-page        
    .collapse.navbar-collapse(collapse="navCollapsed")
      ul.nav.navbar-nav
        li(ng-class="{active: pageName == 'index'}")
          a(href='/index.html') Home
        li(ng-class="{active: pageName == 'page2'}")
          a(href='/page2.html') Page 2
